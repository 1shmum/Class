#include<bits/stdc++.h>
using namespace std;
class Cse
{
public:
    string name;
    int id;
    void printname();
    void printid()
    {
        cout << "Section is: " << id;
    }
};
void Cse::printname()
{
    cout << "Course name is: " << name;
}
int main ()
{
    Cse obj1;
    obj1.name = "OOP";
    obj1.id=2;
    obj1.printname();
    cout << endl;
    obj1.printid();
    return 0;
}
